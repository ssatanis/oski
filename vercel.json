{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/rubricon",
      "dest": "/rubricon.html"
    },
    {
      "src": "/rubricon-clean",
      "dest": "/rubricon-clean.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "*.html": {
      "maxDuration": 10
    }
  }
} 