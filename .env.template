# Azure OpenAI Configuration (Primary)
AZURE_OPENAI_KEY=your_azure_openai_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT_NAME=your_deployment_name

# ChatGPT OpenAI Configuration (Fallback)
CHATGPT_OPENAI_KEY=your_openai_api_key_here

# Debug and CORS settings
DEBUG=True
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000,https://oski-vert.vercel.app,https://oski.app

# Other API Keys (if needed)
GOOGLE_VISION_API_KEY=your_google_vision_key_here
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1
